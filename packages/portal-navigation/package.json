{
  "name": "@inventage-web-components/portal-navigation",
  "version": "0.2.0",
  "description": "A component implementing an opinionated (but generic and hence configurable) navigation pattern.",
  "license": "MIT",
  "author": "Inventage AG <info@inventage.com>",
  "homepage": "https://github.com/inventage/web-components/tree/main/packages/portal-navigation#readme",
  "repository": {
    "url": "inventage/web-components",
    "directory": "packages/portal-navigation"
  },
  "bugs": "https://github.com/inventage/web-components/issues",
  "type": "module",
  "main": "lib/src/index.js",
  "module": "lib/src/index.js",
  "files": [
    "custom-elements.json",
    "lib/src/**/*.d.ts",
    "lib/src/**/*.js",
    "lib/src/**/*.js.map"
  ],
  "scripts": {
    "analyze": "cem analyze --litelement",
    "build": "rimraf lib && tsc -b .",
    "lint": "run-s validate:*",
    "prepublishOnly": "npm run analyze && npm run build",
    "schema": "ts-json-schema-generator --tsconfig '../../tsconfig.json' --path 'src/**/*.ts' --type 'ConfigurationData' --no-top-ref -o configuration.schema-generated.json",
    "test": "echo \"Error: run tests from mono-repo root.\" && exit 1",
    "validate:custom": "ajv validate -s configuration.schema.json -d '../../data/*.json'",
    "validate:generated": "ajv validate -s configuration.schema-generated.json -d '../../data/*.json'"
  },
  "sideEffects": [
    "src/portal-navigation.js"
  ],
  "types": "lib/src/index.d.ts",
  "dependencies": {
    "@inventage-web-components/common": "^0.1.0",
    "@inventage-web-components/hamburger-menu": "^0.2.0",
    "@open-wc/scoped-elements": "^1.3.3"
  },
  "devDependencies": {
    "ajv-cli": "^5.0.0",
    "ts-json-schema-generator": "^0.95.0"
  },
  "keywords": [
    "lit-element",
    "lit-html",
    "portal",
    "web-components"
  ],
  "customElements": "custom-elements.json",
  "exports": {
    ".": "./lib/src/index.js",
    "./portal-navigation": "./lib/src/portal-navigation.js",
    "./portal-navigation.js": "./lib/src/portal-navigation.js"
  }
}
